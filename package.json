{
  "name": "aiaros-fort-knox-dashboard",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"cd apps/voice-dashboard && pnpm dev\" \"cd apps/admin-dashboard && pnpm dev\"",
    "dev:voice": "cd apps/voice-dashboard && pnpm dev",
    "dev:admin": "cd apps/admin-dashboard && pnpm dev",
    "build": "cd apps/voice-dashboard && pnpm build && cd ../admin-dashboard && pnpm build",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@lhci/cli": "^0.13.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.0.0",
    "lint-staged": "^15.0.0",
    "playwright": "1.58.2",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.6.0",
    "standard-version": "^9.5.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "pg": "^8.11.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.0.0"
}
